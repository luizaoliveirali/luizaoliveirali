        "import re, pandas as pd, numpy as np\n",
        "\n",
        "def convert_distance_str(x: str) -> int:\n",
        "    x = str(x)\n",
        "    if \"x\" in x:  # revezamento: \"4x100\" -> 400\n",
        "        a, b = x.split(\"x\")\n",
        "        return int(a) * int(b)\n",
        "    return int(re.sub(\"[^0-9]\", \"\", x))\n",
        "\n",
        "def prever_medalha(\n",
        "    year: int,\n",
        "    distance: str,      # ex: \"100m\" ou \"4x100\"\n",
        "    stroke: str,        # ex: \"Freestyle\", \"Breaststroke\", \"Backstroke\", \"Butterfly\"\n",
        "    relay: int,         # 0=não é revezamento, 1=é revezamento\n",
        "    gender: str,        # \"Men\" ou \"Women\"\n",
        "    team: str,          # ex: \"USA\", \"BRA\", \"AUS\"\n",
        "    model=best_model,   # pipeline já com preprocessor + xgboost (tuned)\n",
        "    threshold: float = 0.40\n",
        "):\n",
        "    df_new = pd.DataFrame([{\n",
        "        \"Year\": year,\n",
        "        \"Distance (in meters)\": distance,\n",
        "        \"Stroke\": stroke,\n",
        "        \"Relay?\": relay,\n",
        "        \"Gender\": gender,\n",
        "        \"Team\": team,\n",
        "        \"Distance_value\": convert_distance_str(distance)\n",
        "    }])\n",
        "    p = model.predict_proba(df_new)[:, 1][0]  # probabilidade da classe 1 (medalha)\n",
        "    yhat = int(p >= threshold)\n",
        "    return {\"prob_medalha\": float(p), \"previsao_binaria\": yhat, \"threshold\": threshold, \"inputs\": df_new.iloc[0].to_dict()}\n"
      ],
      "id": "Ct-tgemrDwc2"
