        "# Exemplos de uso em tabela\n",
        "cenarios = [\n",
        "    dict(year=2016, distance=\"100m\", stroke=\"Freestyle\", relay=0, gender=\"Men\", team=\"USA\"),\n",
        "    dict(year=2020, distance=\"4x100\", stroke=\"Freestyle\", relay=1, gender=\"Women\", team=\"AUS\"),\n",
        "    dict(year=2016, distance=\"200m\", stroke=\"Breaststroke\", relay=0, gender=\"Men\", team=\"BRA\"),\n",
        "]\n",
        "\n",
        "rows = []\n",
        "for c in cenarios:\n",
        "    r = prever_medalha(**c)  # usa o modelo best_model e THRESHOLD padrão (0.40)\n",
        "    rows.append({\n",
        "        \"Year\": c[\"year\"], \"Distance\": c[\"distance\"], \"Stroke\": c[\"stroke\"],\n",
        "        \"Relay\": c[\"relay\"], \"Gender\": c[\"gender\"], \"Team\": c[\"team\"],\n",
        "        \"Prob. medalha\": round(r[\"prob_medalha\"], 3),\n",
        "        \"Decisão (thr=0.40)\": r[\"previsao_binaria\"]\n",
        "    })\n",
        "pd.DataFrame(rows)\n"
